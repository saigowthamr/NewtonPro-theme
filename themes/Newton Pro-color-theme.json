{
  "name": "Newton Pro",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "editor.lineHighlightBackground": "#202020",
    "editor.background": "#222222",
    "editor.selection": "#414141",
    "editor.selectionBackground": "#4d4d4d",
    "editor.selectionHighlightBorder": "#5c5c5c",
    "editor.foreground": "#d4d4d4",
    "editor.whitespaces": "#555555",
	"sideBarBackground": "#292929",


    "tab.background": "#202020",
    "tab.inactiveBackground": "#000929",
	"tab.activeBackground": "#464444",
	"tab.inactiveForeground": "#a0a0a0",


	"statusBarItem.hoverBackground": "#888888",
    "editorCursor.foreground": "#00adfd",
    "editorSuggestWidget.background": "#2d2d2d",
    "editorSuggestWidget.Hoverbackground": "#444444",
    "editorSuggestWidget.border": "#202020",
    "editorSuggestWidget.selectedBackground": "#202020",
    "findRangeHighlightBackground": "#66666699",
    "editor.findMatchBackground": "#101010",
    "editor.findMatchBorder": "#FFD479",

    "button.background": "#4D78CC",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#6087CF",

    "editorLineNumber.foreground": "#606060",
    "editorLineNumber.activeForeground": "#ebebeb",

    "scrollbarSlider.background": "#44444480",
    "scrollbarSlider.activeBackground": "#88888880",
    "scrollbarSlider.hoverBackground": "#77777780",

    "sideBar.background": "#272727",
    "sideBarSectionHeader.background": "#202020",

    "statusBar.background": "#21252B",
    "statusBar.foreground": "#9DA5B4",
    "statusBar.noFolderBackground": "#21252B",
    "statusBar.debuggingForeground": "#FFFFFF",

    "extensionButton.prominentBackground": "#FFD479",
    "extensionButton.prominentForeground": "#222222",
    "extensionButton.prominentHoverBackground": "#ffeea6",
    "badge.background": "#4D78CC",
    "badge.foreground": "#D7DAE0",

    //list
    "list.hoverBackground": "#383838",
    "list.inactiveSelectionBackground": "#444444",
    "list.activeSelectionBackground": "#ac60c4",

    //editor Bracket
    "editorBracketMatch.background": "#9b9b9b83",
    "editorBracketMatch.border": "#6e6e6e",

    "editorError.foreground": "#f4555d",
    "panel.background": "#202020",
    "inputOption.activeBorder": "#4D78CC",
    "inputValidation.warningBorder": "#f4555d",
    "editor.findMatchHighlightBackground": "#cda86955"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#333333",
        "foreground": "#cccccc"
      }
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Text",
      "scope":
        "variable.parameter.function,punctuation.definition.template-expression.begin.js,punctuation.definition.template-expression.end.js"
    },
    {
      "settings": {
        "foreground": "#8a8a80"
      },
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment"
    },
    {
      "settings": {
        "foreground": "#d3d0c8"
      },
      "name": "Punctuation",
      "scope":
        "punctuation.definition.parameters, punctuation.definition.array,punctuation.separator.key-value"
    },
    {
      "settings": {
        "foreground": "#cda869"
      },
      "name": "Punctuation Operators",
      "scope":
        "keyword.operator, constant.character.escape, constant.character.escape.json,entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.pseudo-class.css"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Punctuation Variable",
      "scope": "punctuation.definition.variable"
    },
    {
      "settings": {
        "foreground": "#eeeeee"
      },
      "name": "Object Prop",
      "scope":
        "variable.other.property.php, variable.other.member.php, entity.name.function.php"
    },
    {
      "settings": {
        "foreground": "#d3d0c8"
      },
      "name": "Delimiters",
      "scope": "support.type.property-name.json"
    },
    {
      "settings": {
        "foreground": "#ffd0e6d2"
      },
      "name": "Brackets",
      "scope":
        "punctuation.definition.block,meta.brace.square,meta.parameters,punctuation.definition.parameters,meta.brace.round,punctuation.definition.dictionary,punctuation.definition.array,punctuation.separator,constant.character.escape.backslash.regexp,punctuation.section,punctuation.terminator.statement,variable.other.property.js,support.variable.property.dom.js"
    },
    {
      "settings": {
        "foreground": "#cda869"
      },
      "name": "Operators",
      "scope": "keyword.operator,punctuation.accessor.js"
    },
    {
      "settings": {
        "foreground": "#8ed2ff"
      },
      "name": "CSS Val",
      "scope": "support.constant.property-value"
    },
    {
      "settings": {
        "foreground": "#c5a6ffc0"
      },
      "name": "Keywords",
      "scope":
        "keyword,keyword.operator.expression.typeof,variable.language.this"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Variables",
      "scope": "variable"
    },
    {
      "settings": {
        "foreground": "#ffffff"
      },
      "name": "Function Call",
      "scope": "meta.function-call"
    },
    {
      "settings": {
        "foreground": "#ffb7b7"
      },
      "name": "Functions",
      "scope":
        "entity.name.function, meta.require, support.function.any-method,meta.tag.sgml.html"
    },
    {
      "settings": {
        "foreground": "#78AAD6"
      },
      "name": "Classes",
      "scope": "support.class, entity.name.type.class"
    },
    {
      "settings": {
        "foreground": "#78AAD6"
      },
      "name": "Classes",
      "scope":
        "meta.entity.new.name.expr,entity.name.tag.structure.any.html,meta.tag.structure.any.html,punctuation.definition.tag.html,meta.tag.sgml.doctype.html"
    },
    {
      "settings": {
        "foreground": "#ffffff"
      },
      "name": "Value",
      "scope": "meta.property-value"
    },
    {
      "settings": {
        "foreground": "#78AAD6"
      },
      "name": "Methods",
      "scope": "keyword.other.special-method,entity.name.type"
    },
    {
      "settings": {
        "foreground": "#ffeea6"
      },
      "name": "Storage",
      "scope":
        "storage,support.function.construct.output.php,keyword.operator.new"
    },
    {
      "settings": {
        "foreground": "#D6ACD6"
      },
      "name": "Support",
      "scope": "support.function"
    },
    {
      "settings": {
        "foreground": "#5aa56d"
      },
      "name": "Strings, Inherited Class",
      "scope":
        "string, constant.other.symbol, entity.other.inherited-class,punctuation.definition.string"
    },
    {
      "settings": {
        "foreground": "#fca369"
      },
      "name": "Integers",
      "scope": "constant.numeric"
    },
    {
      "settings": {
        "foreground": "#fca369"
      },
      "name": "Floats",
      "scope": "none"
    },
    {
      "settings": {
        "foreground": "#fca369"
      },
      "name": "Regex",
      "scope": "string.regexp,string.regexp.js"
    },
    {
      "settings": {
        "foreground": "#D6ACD6"
      },
      "name": "Constants",
      "scope": "constant"
    },
    {
      "settings": {
        "foreground": "#dd6363"
      },
      "name": "Tags",
      "scope":
        "entity.name.tag, punctuation.definition.tag.begin.html,punctuation.definition.tag.end.html,support.variable.dom.js,meta.tag.structure.any.html"
    },
    {
      "settings": {
        "foreground": "#FFD479"
      },
      "name": "Attributes",
      "scope":
        "entity.other.attribute-name,entity.other.attribute-name.class.css"
    },
    {
      "settings": {
        "foreground": "#D6ACD6"
      },
      "name": "Attribute IDs",
      "scope": "entity.other.attribute-name.id"
    },
    {
      "settings": {
        "foreground": "#ffffff"
      },
      "name": "Selector",
      "scope": "meta.selector"
    },
    {
      "settings": {
        "foreground": "#dedede"
      },
      "name": "Values",
      "scope":
        "entity.name.tag.reference,meta.embedded.block.php,meta.embedded.line.php"
    },
    {
      "settings": {
        "fontStyle": "",
        "foreground": "#78AAD6"
      },
      "name": "Headings",
      "scope":
        "markup.heading punctuation.definition.heading, entity.name.section"
    },
    {
      "settings": {
        "foreground": "#fca369"
      },
      "name": "Units",
      "scope": "keyword.other.unit"
    },
    {
      "settings": {
        "foreground": "#bbbbbb"
      },
      "name": "Units",
      "scope": "keyword.other.phpdoc.php"
    },
    {
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD479"
      },
      "name": "Bold",
      "scope": "markup.bold, punctuation.definition.bold"
    },
    {
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D6ACD6"
      },
      "name": "Italic",
      "scope": "markup.italic, punctuation.definition.italic"
    },
    {
      "settings": {
        "foreground": "#99cc99"
      },
      "name": "Code",
      "scope": "markup.raw.inline"
    },
    {
      "settings": {
        "foreground": "#99cc99"
      },
      "name": "Link Text",
      "scope": "string.other.link"
    },
    {
      "settings": {
        "background": "#000000",
        "foreground": "#ffffff"
      },
      "name": "Link Url",
      "scope": "meta.link"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Lists",
      "scope": "markup.list"
    },
    {
      "settings": {
        "foreground": "#fca369"
      },
      "name": "Quotes",
      "scope": "markup.quote"
    },
    {
      "settings": {
        "background": "#515151",
        "foreground": "#d3d0c8"
      },
      "name": "Separator",
      "scope": "meta.separator"
    },
    {
      "settings": {
        "foreground": "#99cc99"
      },
      "name": "Inserted",
      "scope": "markup.inserted, markup.inserted.git_gutter"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Deleted",
      "scope": "markup.deleted, markup.deleted.git_gutter"
    },
    {
      "settings": {
        "foreground": "#D6ACD6"
      },
      "name": "Changed",
      "scope": "markup.changed, markup.changed.git_gutter"
    },
    {
      "settings": {
        "foreground": "#515151"
      },
      "name": "Ignored",
      "scope": "markup.ignored, markup.ignored.git_gutter"
    },
    {
      "settings": {
        "foreground": "#515151"
      },
      "name": "Untracked",
      "scope": "markup.untracked, markup.untracked.git_gutter"
    },
    {
      "settings": {
        "foreground": "#76D4D6"
      },
      "name": "Colors",
      "scope":
        "constant.other.color,entity.other.attribute-name.pseudo-class.css, constant.other.php"
    },
    {
      "settings": {
        "foreground": "#76D4D6"
      },
      "name": "Regular Expressions",
      "scope": "string.regexp"
    },
    {
      "settings": {
        "foreground": "#76D4D6"
      },
      "name": "Escape Characters",
      "scope":
        "entity.other.attribute-name.pseudo-element.css,meta.definition.variable.js,meta.var-single-variable.expr.js,variable.other.readwrite.js"
    },
    {
      "settings": {
        "foreground": "#dddddd"
      },
      "name": "Embedded",
      "scope": "punctuation.section.embedded, variable.interpolation"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "Invalid",
      "scope": "invalid.illegal"
    },
    {
      "settings": {
        "foreground": "#f2777a"
      },
      "name": "GitGutter deleted",
      "scope": "markup.deleted.git_gutter"
    },
    {
      "settings": {
        "foreground": "#99cc99"
      },
      "name": "GitGutter inserted",
      "scope": "markup.inserted.git_gutter"
    },
    {
      "settings": {
        "foreground": "#cc99cc"
      },
      "name": "GitGutter changed",
      "scope": "markup.changed.git_gutter"
    },
    {
      "settings": {
        "foreground": "#565656"
      },
      "name": "GitGutter ignored",
      "scope": "markup.ignored.git_gutter"
    },
    {
      "settings": {
        "foreground": "#565656"
      },
      "name": "GitGutter untracked",
      "scope": "markup.untracked.git_gutter"
    }
  ]
}
